<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="at.co.schranz.tlsb.shooter.model.Shooter" table="tlsb_shooter">
		<id name="id" type="java.lang.Integer" unsaved-value="-1">
		    <column name="id" />
		    <generator class="native"/>
		</id>		
		<property name="signIn" type="java.util.Date" column="sign_in"/>
		<property name="lastUpdate" type="java.util.Date" column="last_update"/>
				
		<property name="name" type="string" column="name"/>
		<property name="passNr" type="string" column="pass_nr"/>
		<property name="community" type="string" column="verein"/>
		<property name="sex" type="string" column="geschlecht"/>
		<property name="birthday" type="java.util.Date" column="geburtsdatum"/>
		<property name="armbrust" type="boolean" column="Armbrust"/>
		<property name="gk" type="boolean" column="GK"/>
		<property name="feuerstutzen" type="boolean" column="Feuerstutzen"/>		
		<property name="feuerpistole" type="boolean" column="Feuerpistole"/>			
		<property name="fp" type="boolean" column="FP"/>
		<property name="kk" type="boolean" column="KK"/>
		<property name="lg" type="boolean" column="LG"/>
		<property name="lp" type="boolean" column="LP"/>
		<property name="lp5" type="boolean" column="LP5"/>
		<property name="vorderlader" type="boolean" column="Vorderlader"/>
		<property name="zimmerstutzen" type="boolean" column="Zimmerstutzen"/>
	</class>
</hibernate-mapping>
